{
	"name": "Netstream Lab Dev Container",
	"build": {
		"dockerfile": "Dockerfile"
	},
	"runArgs": [
		"--cap-add=SYS_PTRACE",
		"--security-opt",
		"seccomp=unconfined"
	],
	"customizations": {
		"settings": {
			"terminal.integrated.shell.linux": "/bin/bash"
		},
		"extensions": [
			"ms-vscode.cpptools"
		]
	}
}